{
  "name": "kpi-etl-pipeline",
  "version": "2.0.0",
  "description": "Universal ETL Pipeline for daily KPI metrics from Meta Ads, GA4, and Shopify to Google Sheets",
  "type": "module",
  "main": "dist/server/app.js",
  "scripts": {
    "dev": "npm run tsoa && concurrently \"tsc -w\" \"nodemon --watch dist dist/server/app.js\"",
    "dev:legacy": "concurrently \"tsc -w\" \"nodemon --watch dist dist/server/index.js\"",
    "build": "npm run tsoa && tsc",
    "tsoa": "tsoa spec-and-routes",
    "tsoa:spec": "tsoa spec",
    "tsoa:routes": "tsoa routes",
    "start": "node dist/server/app.js",
    "start:legacy": "node dist/server/index.js",
    "type-check": "tsc --noEmit",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f app",
    "docker:dev": "docker compose --profile dev up -d"
  },
  "keywords": [
    "etl",
    "kpi",
    "google-sheets",
    "meta-ads",
    "ga4",
    "shopify",
    "typescript",
    "bullmq",
    "redis"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.13.23",
    "bullmq": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^12.23.26",
    "google-auth-library": "^9.0.0",
    "googleapis": "^118.0.0",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^7.0.0-alpha.0",
    "uuid": "^9.0.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.5",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  }
}
